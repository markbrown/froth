; Test foldl and foldr from array library

; foldl: left to right
[1 2 3] {print} foldl!

; foldr: right to left
[1 2 3] {print} foldr!

; foldl with transformation
[ [10 20 30] {1 +} foldl! ] 0 @ print
[ [10 20 30] {1 +} foldl! ] 2 @ print

; foldr with transformation
[ [10 20 30] {1 +} foldr! ] 0 @ print
[ [10 20 30] {1 +} foldr! ] 2 @ print

; reverse
[1 2 3] reverse! 0 @ print
[1 2 3] reverse! 1 @ print
[1 2 3] reverse! 2 @ print

; map
[1 2 3] {10 *} map! 0 @ print
[1 2 3] {10 *} map! 1 @ print
[1 2 3] {10 *} map! 2 @ print

; filter
[1 2 3 4 5] {2 >} filter! # print
[1 2 3 4 5] {2 >} filter! 0 @ print
[1 2 3 4 5] {2 >} filter! 1 @ print
[1 2 3 4 5] {2 >} filter! 2 @ print

; reduce
[1 2 3 4] 0 {+} reduce! print
[1 2 3 4] 1 {*} reduce! print
[5] 10 {-} reduce! print

; concat
[1 2] [3 4] concat! # print
[1 2] [3 4] concat! 0 @ print
[1 2] [3 4] concat! 3 @ print
[] [1] concat! # print
[1] [] concat! # print

; flatten
[[1 2] [3 4]] flatten! # print
[[1 2] [3 4]] flatten! 0 @ print
[[1 2] [3 4]] flatten! 3 @ print
[[1] [2] [3]] flatten! # print
[[] [1] []] flatten! # print
