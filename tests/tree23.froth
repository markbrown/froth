; Test 2-3 tree module

; Test empty tree
tree-empty! /t
t tree-is-empty! print " " print
t 5 tree-has! print " " print
t 5 tree-get! print " " print
t tree-size! print " " print

; Test single insert
t 10 "ten" tree-set! /t
t tree-is-empty! print " " print
t 10 tree-has! print " " print
t 10 tree-get! print print " " print
t 5 tree-has! print " " print
t tree-size! print " " print

; Test multiple inserts
t 5 "five" tree-set! /t
t 15 "fifteen" tree-set! /t
t tree-size! print " " print
t 5 tree-get! print print " " print
t 10 tree-get! print print " " print
t 15 tree-get! print print " " print

; Test update existing key
t 10 "TEN" tree-set! /t
t 10 tree-get! print print " " print
t tree-size! print " " print

; Test more inserts to trigger splits
t 3 "three" tree-set! /t
t 7 "seven" tree-set! /t
t 12 "twelve" tree-set! /t
t 20 "twenty" tree-set! /t
t tree-size! print " " print

; Verify all keys present
t 3 tree-has! print
t 5 tree-has! print
t 7 tree-has! print
t 10 tree-has! print
t 12 tree-has! print
t 15 tree-has! print
t 20 tree-has! print " " print

; Test keys in order
t tree-keys! print " " print

; Test values in order
t tree-values! println!
