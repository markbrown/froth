; Test alist module

; Test empty alist
. /a
a alist-empty! print " " print
a alist-size! print " " print

; Test alist-set and alist-get
a 1 "one" alist-set! /a
a 2 "two" alist-set! /a
a 3 "three" alist-set! /a

a 1 alist-get! print print " " print
a 2 alist-get! print print " " print
a 4 alist-get! print " " print

; Test alist-has
a 2 alist-has! print " " print
a 5 alist-has! print " " print

; Test alist-size
a alist-size! print " " print

; Test update existing key
a 2 "TWO" alist-set! /a
a 2 alist-get! print print " " print

; Test alist-delete
a 2 alist-delete! /a
a 2 alist-has! print " " print
a alist-size! print " " print

; Test alist-keys and alist-values
a alist-keys! print " " print
a alist-values! println!
