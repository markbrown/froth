; Test list fold operation

"../lib/list.froth" import

; Build a list [1, 2, 3]
. 3 , 2 , 1 , /xs

; fold with print - should print 1 2 3
xs {print " " print} fold!
nl!

; fold with transform, collect into array
[ xs {1 +} fold! ] println!

; fold to sum elements
0 xs {+} fold! println!

; Empty list fold - should do nothing
. {println!} fold!

; lreverse list [1, 2, 3] to [3, 2, 1]
xs lreverse! {print " " print} fold!
nl!

; lreverse empty list
. lreverse! isNil println!

"done" println!
