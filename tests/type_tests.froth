; Test type-testing operators
; 0 = true, 1 = false

; isInt
{ "\n" println! } /nl

42 isInt println!         ; 0 (true)
"hello" isInt println!    ; 1 (false)

; isString
"hello" isString println! ; 0 (true)
42 isString println!      ; 1 (false)

; isArray
[ 1 2 3 ] isArray println! ; 0 (true)
42 isArray println!        ; 1 (false)

; isMap
$ isMap println!          ; 0 (true)
42 isMap println!         ; 1 (false)

; isIdent
'foo isIdent println!     ; 0 (true)
'/foo isIdent println!    ; 1 (false - it's a binder)

; isBinder
'/bar isBinder println!   ; 0 (true)
'bar isBinder println!    ; 1 (false - it's an ident)

; isFunc
'{ 1 } isFunc println!    ; 0 (true)
'foo isFunc println!      ; 1 (false)

; isGen
'[ 1 ] isGen println!     ; 0 (true)
'foo isGen println!       ; 1 (false)

; isQuote
''foo isQuote println!    ; 0 (true)
'foo isQuote println!     ; 1 (false)

; isApply
'! isApply println!       ; 0 (true)
'foo isApply println!     ; 1 (false)

; isNil
. isNil println!          ; 0 (true)
42 isNil println!         ; 1 (false)

; isCons
. 1 , isCons println!     ; 0 (true)
42 isCons println!        ; 1 (false)

; Closures are cons cells
{ 1 } isCons println!     ; 0 (true - closures are cons pairs)
