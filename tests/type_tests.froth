; Test type-testing builtins
; 0 = true, 1 = false

; isInt
42 isInt print         ; 0 (true)
"hello" isInt print    ; 1 (false)

; isString
"hello" isString print ; 0 (true)
42 isString print      ; 1 (false)

; isArray
[ 1 2 3 ] isArray print ; 0 (true)
42 isArray print        ; 1 (false)

; isMap
$ isMap print          ; 0 (true)
42 isMap print         ; 1 (false)

; isIdent
'foo isIdent print     ; 0 (true)
'/foo isIdent print    ; 1 (false - it's a binder)

; isBinder
'/bar isBinder print   ; 0 (true)
'bar isBinder print    ; 1 (false - it's an ident)

; isFunc
'{ 1 } isFunc print    ; 0 (true)
'foo isFunc print      ; 1 (false)

; isGen
'[ 1 ] isGen print     ; 0 (true)
'foo isGen print       ; 1 (false)

; isQuote
''foo isQuote print    ; 0 (true)
'foo isQuote print     ; 1 (false)

; isApply
'! isApply print       ; 0 (true)
'foo isApply print     ; 1 (false)

; isNil
. isNil print          ; 0 (true)
42 isNil print         ; 1 (false)

; isCons
. 1 , isCons print     ; 0 (true)
42 isCons print        ; 1 (false)

; Closures are cons cells
{ 1 } isCons print     ; 0 (true - closures are cons pairs)
