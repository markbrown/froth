; Test write operator - outputs executable code
; Run with -n to avoid stdlib pollution in closure output

"../lib/io.froth" import

; Integers

42 writeln!
-5 writeln!

; Strings (with escapes)
"hello" writeln!
"say \"hi\"" writeln!
"line1\nline2" writeln!

; Arrays
[1 2 3] writeln!
[] writeln!

; Nil
. writeln!

; Cons lists
. 3 , 2 , 1 , writeln!

; Terms
'foo writeln!
'{ 1 2 + } writeln!

; Closures (dummy output)
{ 1 2 + } writeln!
