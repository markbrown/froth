; Test delete-keys and add-keys functions

; === delete-keys ===

; Basic deletion
$ 1 'a : 2 'b : 3 'c : ['a 'c] delete-keys! write nl!
; $ 2 'b :

; Delete single key
$ 1 'x : 2 'y : ['x] delete-keys! write nl!
; $ 2 'y :

; Delete from empty map
$ ['x 'y] delete-keys! write nl!
; $

; Delete non-existent keys
$ 1 'a : ['x 'y] delete-keys! write nl!
; $ 1 'a :

; Delete all keys
$ 1 'a : 2 'b : ['a 'b] delete-keys! write nl!
; $

; Empty keys array
$ 1 'a : 2 'b : [] delete-keys! write nl!
; $ 1 'a : 2 'b :

; === add-keys ===

; Add to empty map
$ ['a 'b] add-keys! write nl!
; $ . 'a : . 'b :

; Add to existing map
$ 1 'x : ['a 'b] add-keys! write nl!
; $ 1 'x : . 'a : . 'b :

; Add overlapping keys (nil overwrites)
$ 1 'a : 2 'b : ['b 'c] add-keys! write nl!
; $ 1 'a : . 'b : . 'c :

; Empty keys array
$ 1 'a : [] add-keys! write nl!
; $ 1 'a :
