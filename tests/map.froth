; Test map operations

; $ creates an empty map
$ # print

; store values in map
$ 42 'x : 10 'y : /m

; get map size
m # print

; lookup values
m 'x @ print
m 'y @ print

; keys returns array of keys
m keys # print

; in tests key existence
m 'x in print   ; 0 (key exists)
m 'z in print   ; 1 (key doesn't exist)

; map equality
$ 1 'a : 2 'b :
$ 2 'b : 1 'a :
= print

; write outputs executable form
$ 1 'x : write
