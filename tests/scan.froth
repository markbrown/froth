; Test scanl, scanr, and contains

; scanl - find element
[1 2 3] {3 =} scanl! println!    ; 0 (found)
[1 2 3] {5 =} scanl! println!    ; 1 (not found)

; scanl - empty array
[] {1 =} scanl! println!         ; 1 (not found)

; scanl - stop early
[1 2 3 4 5] {3 >} scanl! println! ; 0 (found 4)

; scanr - find element
[1 2 3] {1 =} scanr! println!    ; 0 (found)
[1 2 3] {5 =} scanr! println!    ; 1 (not found)

; scanr - empty array
[] {1 =} scanr! println!         ; 1 (not found)

; contains - found
[1 2 3] 2 contains! println!     ; 0 (found)
["a" "b" "c"] "b" contains! println! ; 0 (found)

; contains - not found
[1 2 3] 5 contains! println!     ; 1 (not found)
["a" "b"] "c" contains! println! ; 1 (not found)

; contains - empty array
[] 1 contains! println!          ; 1 (not found)
