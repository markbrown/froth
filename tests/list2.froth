; Test lconcat and lmap

; lconcat - basic
. 2 , 1 , . 4 , 3 , lconcat! {println!} lfoldl!
; prints: 1 2 3 4

; lconcat - first empty
. . 3 , 2 , 1 , lconcat! {println!} lfoldl!
; prints: 1 2 3

; lconcat - second empty
. 3 , 2 , 1 , . lconcat! {println!} lfoldl!
; prints: 1 2 3

; lconcat - both empty
. . lconcat! isNil println!
; 0 (is nil)

; lmap - basic
. 3 , 2 , 1 , {1 +} lmap! {println!} lfoldl!
; prints: 2 3 4

; lmap - double
. 3 , 2 , 1 , {2 *} lmap! {println!} lfoldl!
; prints: 2 4 6

; lmap - empty list
. {1 +} lmap! isNil println!
; 0 (is nil)
